<?xml version="1.0" encoding="UTF-8"?>
<configuration status="warn" monitorInterval="1800">

    <Appenders>
        <Console name="console" target="SYSTEM_OUT">
            <PatternLayout pattern="%-5p %d - %msg @ %l%n" />
        </Console>

    </Appenders>
    
    <loggers>
        <root level="debug">
            <appender-ref ref="console" />
        </root>
    </loggers>
</configuration>